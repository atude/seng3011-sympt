{
  "name": "sympt-core",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "heroku-prebuild": "npm run setup:all",
    "setup:all": "rm -rf ./node_modules && npm install && npm run setup:server && npm run setup:web",
    "setup:server": "cd server && npm run setup",
    "setup:web": "cd web && npm run setup",
    "start:server": "cd server && npm start",
    "start:web": "cd web && npm start",
    "start": "concurrently npm:start:*",
    "lint:check": "cd server && npm run lint:check -s && cd ../web && npm run lint:check -s",
    "lint:fix": "cd server && npm run lint:fix -s && cd ../web && npm run lint:fix -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atude/seng3011-sympt.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atude/seng3011-sympt/issues"
  },
  "homepage": "https://github.com/atude/seng3011-sympt#readme",
  "dependencies": {
    "concurrently": "^5.1.0"
  },
  "devDependencies": {
    "husky": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:check"
    }
  }
}
